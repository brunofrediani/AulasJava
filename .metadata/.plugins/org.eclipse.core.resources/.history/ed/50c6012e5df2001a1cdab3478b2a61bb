package entities;

import java.util.Date;

public class Order{

	private Integer id;
	private Date moment;
	private entities.enums.OrderStatus status;
	
	public Order() {
	}

	public <OrderStatus> Order(Integer id, Date moment, entities.enums.OrderStatus pendingPayment) {
		this.id = id;
		this.moment = moment;
		this.status = pendingPayment;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Date getMoment() {
		return moment;
	}

	public void setMoment(Date moment) {
		this.moment = moment;
	}

	public entities.enums.OrderStatus getStatus() {
		return status;
	}

	public void setStatus(entities.enums.OrderStatus status) {
		this.status = status;
	}

	@Override
	public String toString() {
		return "Order [id=" + id + ", moment=" + moment + ", status=" + status + "]";
	}

}
